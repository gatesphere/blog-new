  p = os.path.join(scandir, y)
  months = os.listdir(p)
  
  for m in months:
    p = os.path.join(scandir,y, m)
    days = os.listdir(p)
    if len(days) == 0:
      os.rmdir(p)
      g.es('deleted %s' % p)

